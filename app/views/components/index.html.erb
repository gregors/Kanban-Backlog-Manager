<h1>Components</h1>


<table id="componentstable" class="stories" cellpadding="0" cellspacing="0">
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Stories</th>
  </tr>
<%@components.each do |c|%>
  <tr>
    <td><%=c.title%></td>
    <td><%=c.description%></td>
    <td><%#=link_to %></td>
  </tr>
<%end%>
</table>